{
  "name": "cultureamp-style-guide",
  "version": "2.2.0",
  "description": "Culture Amp Living Style Guide",
  "main": "index.js",
  "repository": "git@github.com:cultureamp/cultureamp-style-guide.git",
  "author": "Kevin Yank <kevin@cultureamp.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:assets && npm run build:site",
    "build:assets": "webpack --config guide/webpack.config.js",
    "build:site": "cd guide && JEKYLL_ENV=production bundle exec jekyll build --baseurl \"$npm_package_config_baseurl\"",
    "clean": "rm -rf guide/assets/* guide/dist/*",
    "gh-pages": "npm run clean && npm run gh-pages:build && npm run gh-pages:deploy",
    "gh-pages:build": "npm run gh-pages:build:assets && npm run gh-pages:build:site",
    "gh-pages:build:assets": "npm run build:assets",
    "gh-pages:build:site": "npm run build:site --cultureamp-style-guide:baseurl=\"/cultureamp-style-guide\"",
    "gh-pages:deploy": "scripts/gh-pages-deploy.js",
    "start": "concurrently --kill-others \"npm run start:assets\" \"npm run start:site\"",
    "start:assets": "NODE_ENV=development webpack-dev-server -d --config guide/webpack.config.js",
    "start:site": "cd guide && bundle exec jekyll serve --watch"
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions",
    "Chrome >= 42",
    "IE >= 10",
    "Edge >= 12",
    "Firefox >= 38",
    "Safari >= 5",
    "Android >= 4"
  ],
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "classnames": "^2.2.5",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-normalize": "^3.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-add-react-displayname": "^0.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "react-html-id": "^0.1.0",
    "svg-sprite-loader": "^3.0.5",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1"
  },
  "peerDependencies": {
    "babel-polyfill": "^6.23.0",
    "extract-text-webpack-plugin": "^2.1.2 || ^3.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "webpack": "^2.6.1 || ^3.1.0"
  }
}
